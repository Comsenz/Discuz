{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    "transform-es2015-modules-commonjs",
    "transform-vue-jsx",
    "transform-runtime",
    [
      "import", {
      "libraryName": "vant",
      "libraryDirectory": "es"
    },
      "component", {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      }
    ]
  ]
}


