{
    "name": "comsenz/discuss",
    "description": "The Discuz software open-source.",
    "type": "project",
    "keywords": ["discuz", "comsenz", "forum", "community"],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "discuz/core": "dev-master"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "./framework"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "symfony/var-dumper": "4.4.x-dev"
    },
    "autoload": {
        "psr-4": {
            "App\\": "./app"
        }
    }
}
